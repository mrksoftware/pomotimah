{\n  \"name\": \"pomotimah\",\n  \"version\": \"0.2.2-beta1\",\n  \"author\": \"Mirko Bellabarba <mirko.bellabarba@gmail.com>\",\n  \"description\": \"Pomodoro timer built in vue and electron\",\n  \"license\": null,\n  \"main\": \"./dist/electron/main.js\",\n  \"repository\": \"git@github.com:mrksoftware/pomotimah.git\",\n  \"scripts\": {\n    \"build\": \"node .electron-vue/build.js && electron-builder\",\n    \"build:dir\": \"node .electron-vue/build.js && electron-builder --dir\",\n    \"build:clean\": \"cross-env BUILD_TARGET=clean node .electron-vue/build.js\",\n    \"build:web\": \"cross-env BUILD_TARGET=web node .electron-vue/build.js\",\n    \"dev\": \"node .electron-vue/dev-runner.js\",\n    \"dist\": \"electron-builder\",\n    \"lint\": \"eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test\",\n    \"lint:fix\": \"eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test\",\n    \"pack\": \"npm run pack:main && npm run pack:renderer\",\n    \"pack:main\": \"cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js\",\n    \"pack:renderer\": \"cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js\",\n    \"start\": \"electron .\",\n    \"ship\": \"node .electron-vue/build.js && electron-builder --win -p always\",\n    \"test\": \"npm run unit\",\n    \"unit\": \"karma start test/unit/karma.conf.js\",\n    \"postinstall\": \"npm run lint:fix\"\n  },\n  \"build\": {\n    \"productName\": \"pomotimah\",\n    \"appId\": \"org.mrksoftware.pomotimah\",\n    \"directories\": {\n      \"output\": \"build\"\n    },\n    \"files\": [\n      \"dist/electron/**/*\"\n    ],\n    \"win\": {\n      \"icon\": \"build/icons/icon.ico\",\n      \"target\": [\n        {\n          \"target\": \"nsis\",\n          \"arch\": [\n            \"x64\",\n            \"ia32\"\n          ]\n        }\n      ]\n    },\n    \"publish\": {\n      \"provider\": \"github\",\n      \"repo\": \"pomotimah\",\n      \"owner\": \"mrksoftware\"\n    }\n  },\n  \"dependencies\": {\n    \"axios\": \"^0.18.0\",\n    \"electron-updater\": \"^2.21.0\",\n    \"vue\": \"^2.5.13\",\n    \"vue-electron\": \"^1.0.6\",\n    \"vue-router\": \"^3.0.1\",\n    \"vuex\": \"^3.0.1\"\n  },\n  \"devDependencies\": {\n    \"babel-core\": \"^6.25.0\",\n    \"babel-loader\": \"^7.1.1\",\n    \"babel-plugin-transform-runtime\": \"^6.23.0\",\n    \"babel-preset-env\": \"^1.6.0\",\n    \"babel-preset-stage-0\": \"^6.24.1\",\n    \"babel-register\": \"^6.24.1\",\n    \"babili-webpack-plugin\": \"^0.1.2\",\n    \"cfonts\": \"^1.1.3\",\n    \"chalk\": \"^2.1.0\",\n    \"copy-webpack-plugin\": \"^4.0.1\",\n    \"cross-env\": \"^5.0.5\",\n    \"css-loader\": \"^0.28.4\",\n    \"del\": \"^3.0.0\",\n    \"devtron\": \"^1.4.0\",\n    \"electron\": \"2.0.0-beta.1\",\n    \"electron-debug\": \"^1.4.0\",\n    \"electron-devtools-installer\": \"^2.2.0\",\n    \"electron-builder\": \"^20.2.0\",\n    \"electron-is-dev\": \"^0.3.0\",\n    \"babel-eslint\": \"^7.2.3\",\n    \"eslint\": \"^4.4.1\",\n    \"eslint-friendly-formatter\": \"^3.0.0\",\n    \"eslint-loader\": \"^1.9.0\",\n    \"eslint-plugin-html\": \"^3.1.1\",\n    \"eslint-config-standard\": \"^10.2.1\",\n    \"eslint-plugin-import\": \"^2.7.0\",\n    \"eslint-plugin-node\": \"^5.1.1\",\n    \"eslint-plugin-promise\": \"^3.5.0\",\n    \"eslint-plugin-standard\": \"^3.0.1\",\n    \"extract-text-webpack-plugin\": \"^3.0.0\",\n    \"file-loader\": \"^0.11.2\",\n    \"html-webpack-plugin\": \"^2.30.1\",\n    \"inject-loader\": \"^3.0.0\",\n    \"karma\": \"^1.3.0\",\n    \"karma-chai\": \"^0.1.0\",\n    \"karma-coverage\": \"^1.1.1\",\n    \"karma-electron\": \"^5.1.1\",\n    \"karma-mocha\": \"^1.2.0\",\n    \"karma-sourcemap-loader\": \"^0.3.7\",\n    \"karma-spec-reporter\": \"^0.0.31\",\n    \"karma-webpack\": \"^2.0.1\",\n    \"webpack-merge\": \"^4.1.0\",\n    \"babel-plugin-istanbul\": \"^4.1.1\",\n    \"chai\": \"^4.0.0\",\n    \"mocha\": \"^3.0.2\",\n    \"multispinner\": \"^0.2.1\",\n    \"node-loader\": \"^0.6.0\",\n    \"node-sass\": \"^4.7.2\",\n    \"style-loader\": \"^0.18.2\",\n    \"sass-loader\": \"^6.0.7\",\n    \"url-loader\": \"^0.5.9\",\n    \"vue2-circle-progress\": \"^1.2.1\",\n    \"vue-html-loader\": \"^1.2.4\",\n    \"vue-loader\": \"^13.0.5\",\n    \"vue-style-loader\": \"^3.0.1\",\n    \"vue-template-compiler\": \"^2.4.2\",\n    \"webpack\": \"^3.5.2\",\n    \"webpack-dev-server\": \"^2.7.1\",\n    \"webpack-hot-middleware\": \"^2.18.2\"\n  }\n}